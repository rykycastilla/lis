{
  "compilerOptions": {
    "target": "es2016",
    "module": "CommonJS",
    "esModuleInterop": true,
    "declaration": true,
    "rootDir": "./src",
    "outDir": "./dist",
    "noImplicitReturns": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "skipLibCheck": true,
    "removeComments": true
  },
}